        <TickValuesFont>
            <Name>Helvetica</Name>
            <Style>PLAIN</Style>
            <Size>10</Size>
            <Color r="0" g="0" b="255"/>
        </TickValuesFont>
        <LegendFont>
            <Name>SanSerif</Name>
            <Style>PLAIN</Style>
            <Size>10</Size>
            <Color r="255" g="255" b="255"/>
        </LegendFont>
    </Text>
    <PlotElements>
        <Background>
            <Color r="216" g="216" b="216"/>
        </Background>
        <Grid>
            <Visibility x="false"/>
            <Color r="175" g="175" b="175"/>
        </Grid>
        <Axis>
            <Color r="65" g="65" b="65"/>
        </Axis>
        <Series>
            <ColorPaletteArray>matplotlib</ColorPaletteArray>
