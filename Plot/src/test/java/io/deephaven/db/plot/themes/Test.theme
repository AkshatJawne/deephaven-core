<Theme>
    <Name>Test</Name>
    <Text>
        <FigureTextFont>
            <Name>SanSerif</Name>
            <Style>PLAIN</Style>
            <Size>14</Size>
            <Color r="65" g="65" b="65"/>
        </FigureTextFont>
        <FigureTitleFont>
            <Name>Arial</Name>
            <Style>BOLD</Style>
            <Size>28</Size>
            <Color r="0" g="0" b="0"/>
